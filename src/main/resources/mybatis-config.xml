<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<typeAlias type="com.air.future.vo.Admin" alias="admin"/>
		<typeAlias type="com.air.future.vo.Customer" alias="customer"/>
		<typeAlias type="com.air.future.vo.Grade" alias="grade"/>
		<typeAlias type="com.air.future.vo.Post" alias="post"/>
		<typeAlias type="com.air.future.vo.Route" alias="route"/>
		<typeAlias type="com.air.future.vo.Destination" alias="destination"/>
		<typeAlias type="com.air.future.vo.Board" alias="board"/>
		<typeAlias type="com.air.future.vo.Airplane" alias="airplane"/>
	</typeAliases>
	
	<mappers>
	<mapper resource="mapper/AdminMapper.xml"/>
	<mapper resource="mapper/AdminFlightMapper.xml"/>
	<mapper resource="mapper/AdminCustomerMapper.xml"/>
	<mapper resource="mapper/AdminBoardMapper.xml"/>
	</mappers>
</configuration>